{
    "python.linting.enabled": true,
    "python.linting.pycodestyleEnabled": true,
    "python.linting.pycodestyleArgs": [
      "--ignore=",
      "--max-line-length=119",
      "--exclude=**/migrations/**",
    ],
    "python.workspaceSymbols.exclusionPatterns": [
      "**/site-packages/**",
      "**/migrations/**",
    ]
}
